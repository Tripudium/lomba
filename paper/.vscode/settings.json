{
  "files.associations": { "*.tex": "latex" },
  "editor.wordWrap": "on",
  "editor.formatOnSave": false,
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-pdf",
        "-interaction=nonstopmode",
        "%DOC%"
      ]
    }
  ],
  "latex-workshop.latex.recipe.default": "latexmk",
  "latex-workshop.view.pdf.viewer": "tab",
  "cSpell.words": [
    "Hadwiger"
  ]
}